{
  "name": "CoFrame",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "tauri dev",
    "start:web": "vite",
    "dev": "vite",
    "devtest": "vite --config ./vite-test.config.js",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:tauri": "tauri build --debug",
    "build:wasm": "wasm-pack build --release ./coframe-rust --target web",
    "test:wasm": "cargo test --release ./coframe-rust",
    "build:gh": "vite build && git add dist -f && git commit -m \"Updating gh-pages\" && git subtree push --prefix dist origin gh-pages",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "5.13.5",
    "@people_and_robots/lively": "^1.1.0",
    "@radix-ui/react-scroll-area": "^1.0.0",
    "@robostack/roslib": "^1.1.1",
    "@tauri-apps/api": "^1.3.0",
    "@visx/visx": "^3.1.2",
    "comlink": "^4.4.1",
    "fast-xml-parser": "^4.0.9",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.10.0",
    "immer": "^9.0.15",
    "number-precision": "^1.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-json-view": "^1.21.3",
    "react-markdown": "^8.0.4",
    "react-reflex": "^4.0.9",
    "react-remark": "^2.1.0",
    "react-use-measure": "^2.1.1",
    "redux": "^4.2.0",
    "robot-scene": "github:Wisc-HCI/robot-scene#v0.9.10",
    "simple-vp": "github:Wisc-HCI/simple-vp#v0.8.11",
    "three": "^0.142.0",
    "uuid": "^8.3.2",
    "worker-loader": "^3.0.8",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.3.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "vite": "^4.2.1",
    "vite-plugin-comlink": "^3.0.5",
    "vite-plugin-svgr": "^2.2.0",
    "vite-plugin-top-level-await": "^1.2.2",
    "vite-plugin-wasm": "^3.2.1",
    "vite-plugin-wasm-pack": "^0.1.12",
    "vitest": "^0.28.5"
  }
}
