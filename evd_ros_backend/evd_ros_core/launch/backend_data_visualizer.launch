<<?xml version="1.0" encoding="UTF-8"?>
<launch>

  <arg name="ros_frame_id"/>

  <node name="program_model_rviz_publisher" pkg="evd_ros_core" type="program_model_rviz_publisher.py" output="screen">
    <param name="ros_frame_id" value="$(arg ros_frame_id)"/>
  </node>

  <node name="environment_model_rviz_publisher" pkg="evd_ros_core" type="environment_model_rviz_publisher.py" output="screen">
    <param name="ros_frame_id" value="$(arg ros_frame_id)"/>
  </node>

  <node name="application_model_control_publisher" pkg="evd_ros_core" type="application_model_control_publisher.py" output="screen">
    <param name="ros_frame_id" value="$(arg ros_frame_id)"/>
  </node>

</launch>
